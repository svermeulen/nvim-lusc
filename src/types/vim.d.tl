-- vim: ft=teal expandtab sw=3 ts=3

global record vim
   record SystemTime
   end

   record QuickFixItem
      bufnr:integer
      module:string
      lnum:integer
      end_lnum:integer
      col:integer
      end_col:integer
      vcol:boolean
      nr:integer
      pattern:string
      text:string
      type:string
      valid:boolean
