   -- Sadly, there doesn't seem to be a nice way to serialize all of these
   -- since they're lazily brought in as you call them
   -- This also will never be complete because user defined functions are also called here
   --     ex vim.fn["MyPlugin#MyFunction"]()
   -- We could represent this if we had 'record maps' in teal but this doesn't exist:
   --     VIM_fn = record
   --        {string:function(...: any): (any)}
   --        mode: function(): Mode
   --        ...
   --     end
   record Fn
      -- File management
      filereadable:function(string):integer
      isdirectory:function(string):integer
      delete:function(string, string):integer
      mkdir:function(string, string):integer
      readfile:function(string, string):{string}
      writefile:function({string}, string, string)
      getftime:function(string):integer
      getfsize:function(string):integer
      globpath:function(string, string, integer, integer):{string}
      input:function(string, string):string
      setqflist: function({string:any})
      mode: function(full:boolean):string
      getcurpos: function(winid:integer):{any}
      cursor: function(lnum:integer, col:integer, offset:integer)
      cursor: function(position:{any})
      search: function(string, string, integer, number)
      sign_define: function(string, {string:any})
      strftime: function(string):string
      getpos: function(string):{any}
      resolve: function(string):string
      getpid: function():integer
      getcwd: function(winnr:integer, tabnr:integer):string
      complete:function(integer, {string})
      getqflist: function({string:any}):{QuickFixItem}

      enum LineExpr
         "."
         "$"
         "'x"
         "w0"
         "w$"
         "v"
         "'>"
      end

      line: function(expr:LineExpr, win_id:integer):integer
      col: function(expr:LineExpr, win_id:integer):integer

      getline:function(lnum:integer, endline:integer):string|{string}

      setpos:function(expr:LineExpr, position:{any})
      bufnr:function(buf:string, create:boolean):integer

      -- We'd prefer to use this but 'repeat' is a reserved word
      -- record TimerOptions
      --    repeat:integer
      -- end
      -- timer_start:function(integer, function(), TimerOptions)
      timer_start:function(integer, function(), {string:any}):integer

      timer_stop:function(integer)

      system: function(string):string

      enum ExecuteSilentOption
         ""
         "silent"
         "silent!"
      end

      execute: function(string|{string}, ExecuteSilentOption):string

      record JobOptions
         clear_env:boolean
         cwd:string
         detach:boolean
         env:{string:string}
         height:number
         on_exit:function(integer, integer, string)
         on_stdout:function(integer, {string}, string)
         on_stderr:function(integer, {string}, string)
         overlapped:boolean
         pty:boolean
         rpc:boolean
         stderr_buffered:boolean
         stdout_buffered:boolean
         width:number
      end

      jobstart:function(string|{string}, JobOptions):integer
      jobwait:function({integer}):{integer}

      enum TabpagenrArg
         "$"
         "#"
      end

      tabpagenr:function(TabpagenrArg):integer
      tabpagebuflist:function(integer):{integer}

      termopen:function(string | {string}, JobOptions):integer

      executable:function(string):integer

      -- String manipulation
      escape:function(string, string):string
      expand:function(string):string

      -- Other
      getreg:function(string):string
      setreg:function(string, string)

      reltime:function(SystemTime):SystemTime
      reltimestr:function(SystemTime):string
      reltimefloat:function(SystemTime):number

      confirm:function(string)
   end

   fn: Fn

   record Treesitter
      get_captures_at_cursor:function():{string}
      -- TODO - the rest
   end

   treesitter: Treesitter

   cmd: function(cmd: string)
   call: function(func: string, ...: any):any
   call: function(string, ...: any): any

   enum Mode
      "n" -- Normal
      "no" -- Operator-pending
      "nov" -- Operator-pending (forced charwise |o_v|)
      "noV" -- Operator-pending (forced linewise |o_V|)
      "no" -- Operator-pending (forced blockwise |o_CTRL-V|)
      "niI" -- Normal using |i_CTRL-O| in |Insert-mode|
      "niR" -- Normal using |i_CTRL-O| in |Replace-mode|
      "niV" -- Normal using |i_CTRL-O| in |Virtual-Replace-mode|
      "v" -- Visual by character
      "V" -- Visual by line
      "" -- Visual blockwise
      "s" -- Select by character
      "S" -- Select by line
      "" -- Select blockwise
      "i" -- Insert
      "ic" -- Insert mode completion |compl-generic|
      "ix" -- Insert mode |i_CTRL-X| completion
      "R" -- Replace |R|
      "Rc" -- Replace mode completion |compl-generic|
      "Rv" -- Virtual Replace |gR|
      "Rx" -- Replace mode |i_CTRL-X| completion
      "c" -- Command-line editing
      "cv" -- Vim Ex mode |gQ|
      "ce" -- Normal Ex mode |Q|
      "r" -- Hit-enter prompt
      "rm" -- The -- more -- prompt
      "r?" -- |:confirm| query of some sort
      "!" -- Shell or external command is executing
      "t" -- Terminal mode: keys go to the job
   end

   record InspectOptions
      depth: integer
      newline: string
      indent: string
      process: function
   end
   inspect: function(any, InspectOptions): string

   record log
      record levels
         TRACE: integer
         DEBUG: integer
         INFO: integer
         HINT: integer
         WARN: integer
         ERROR: integer
         OFF: integer
      end
   end
